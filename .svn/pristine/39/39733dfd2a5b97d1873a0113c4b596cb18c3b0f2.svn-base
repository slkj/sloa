<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
</head>
<body>
	<form id="postForm" metdod="post">
		<input id="id" name="id" type="hidden" /> 
		<input id="vId" name="vId" type="hidden" /> 
		<table class="grid" style="width: 100%">
		<tr>
				<th width="100px">变更类型：</th>
				<td  colspan='3'><label> <input name="transferType" type="radio" value="1" checked="checked" />车辆过户
				</label> <label><input name="transferType" type="radio" value="2" />牌照变更</label></td>	
			</tr>
			<tr>
				<th style="width: 120px">原车牌号：</th>
				<td><input id="carNumber" name="oldCarNumber" class="easyui-textbox" 
						style="width: 200px"></td>
				<th style="width: 120px">现车牌号：</th>
				<td><input name="newCarNumber" class="easyui-textbox" 
						style="width: 200px"></td>
			</tr>
			<tr>

				<th>原设备号：</th>
				<td><input id="oldDevice1" name="oldDevice" class="easyui-textbox" style="width: 200px" /></td>
				<th>现设备号：</th>
				<td><input name="newDevice" class="easyui-textbox" style="width: 200px"></td>
				
			</tr>
			<tr>
				<th>原SIM卡号：</th>
				<td><input id="oldSIM1" name="oldSIM" class="easyui-textbox" style="width: 200px"></td>
				<th>现SIM卡号：</th>
				<td><input name="newSIM" class="easyui-textbox" style="width: 200px"> <a class="imgInc"></a></td>
			</tr>
			<tr>
				<th>原所属公司：</th>
				<td><input id="oldCompany1" name="oldCompany" class="easyui-textbox" style="width: 200px"></td>
				<th>现所属公司：</th>
				<td><input name="newCompany" class="easyui-textbox" style="width: 200px"> <a class="imgInc"></a></td>
			</tr>
			<tr>
				<th>原车主：</th>
				<td><input id="oldOwner1" name="oldOwner" class="easyui-textbox" style="width: 200px"></td>
				<th>现车主：</th>
				<td><input name="newOwner" class="easyui-textbox" style="width: 200px"> <a class="imgInc"></a></td>
			</tr>
			<tr>
				<th>原联系方式：</th>
				<td><input id="oldPhone1" name="oldPhone" class="easyui-textbox" style="width: 200px"></td>
				<th>现联系方式：</th>
				<td><input name="newPhone" class="easyui-textbox" style="width: 200px"> <a class="imgInc"></a></td>
			</tr>
			<tr>
				<th>日期：</th>
				<td colspan='3'><input name="riqi" class="Wdate" type="text" style="width: 200px;"
						onFocus="WdatePicker({isShowClear:false,readOnly:true})" ></td>				
			</tr>	
		</table>
	</form>
</body>
</html>