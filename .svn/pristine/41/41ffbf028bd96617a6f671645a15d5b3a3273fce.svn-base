package cn.slkj.sloa.entity.shfw;

public class Driver {
	private String id;
	private String carNumber;// 车牌号
	private String deviceNum;// 设备号
	private String driverNo;// 司机卡号
	private String driverName; // 司机姓名
	private String drivingNo;// 驾驶证号
	private String madeTime; // 制作日期
	private String collar;// 领用人
	private String companyId;// 公司
	private String companyName;// 公司
	private String remark;// 备注

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCarNumber() {
		return carNumber;
	}

	public void setCarNumber(String carNumber) {
		this.carNumber = carNumber;
	}

	public String getDeviceNum() {
		return deviceNum;
	}

	public void setDeviceNum(String deviceNum) {
		this.deviceNum = deviceNum;
	}

	public String getDriverNo() {
		return driverNo;
	}

	public void setDriverNo(String driverNo) {
		this.driverNo = driverNo;
	}

	public String getDriverName() {
		return driverName;
	}

	public void setDriverName(String driverName) {
		this.driverName = driverName;
	}

	public String getDrivingNo() {
		return drivingNo;
	}

	public void setDrivingNo(String drivingNo) {
		this.drivingNo = drivingNo;
	}

	public String getMadeTime() {
		return madeTime;
	}

	public void setMadeTime(String madeTime) {
		this.madeTime = madeTime;
	}

	public String getCollar() {
		return collar;
	}

	public void setCollar(String collar) {
		this.collar = collar;
	}

	public String getCompanyId() {
		return companyId;
	}

	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}

	public String getCompanyName() {
		return companyName;
	}

	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	@Override
	public String toString() {
		return "Driver [id=" + id + ", carNumber=" + carNumber + ", deviceNum=" + deviceNum + ", driverNo=" + driverNo + ", driverName=" + driverName + ", drivingNo=" + drivingNo + ", madeTime=" + madeTime + ", collar=" + collar + ", companyId=" + companyId + ", companyName=" + companyName + ", remark=" + remark + "]";
	}

}
